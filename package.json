{
  "name": "yareq",
  "version": "1.0.2",
  "keywords": [
    "request",
    "proxy",
    "socks",
    "https"
  ],
  "description": "Yet another request for Node.js",
  "author": "Weidong Fang",
  "license": "MIT",
  "homepage": "https://github.com/fangwd/yareq",
  "main": "./dist/index.js",
  "files": [
    "src",
    "dist",
    "LICENSE",
    "package.json",
    "README.md"
  ],
  "types": "./dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fangwd/yareq.git"
  },
  "dependencies": {
    "socks-wrapper": "^0.4.0"
  },
  "devDependencies": {
    "@types/jest": "^23.3.12",
    "@types/node": "^10.12.18",
    "@types/tough-cookie": "^2.3.4",
    "jest": "^23.6.0",
    "tough-cookie": "^2.4.2",
    "ts-jest": "^23.10.5",
    "typescript": "^3.2.2"
  },
  "scripts": {
    "test": "jest"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "/test/response\\.test.ts",
    "testURL": "http://localhost/",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "collectCoverage": false,
    "collectCoverageFrom": [
      "src/**/*.ts"
    ],
    "verbose": true
  },
  "bugs": {
    "url": "https://github.com/fangwd/yareq/issues"
  }
}
